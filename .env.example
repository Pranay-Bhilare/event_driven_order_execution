# Copy this to .env and fill values. Never commit .env.
REDIS_URL=redis://localhost:6379/0
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ingestion

# Optional: artificial DB delay in worker (ms) for backpressure tests, e.g. 200
# DB_SLOWDOWN_MS=0

# Optional: AWS SQS (when set, API and worker use SQS instead of Redis queue)
# AWS_ACCESS_KEY_ID=...
# AWS_SECRET_ACCESS_KEY=...
# AWS_REGION=ap-south-1
# SQS_QUEUE_URL=https://sqs.<region>.amazonaws.com/<account>/ingestion-events
# SQS_DLQ_URL=https://sqs.<region>.amazonaws.com/<account>/ingestion-events-dlq
